		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<List items="{/V_Team}">
							<StandardListItem type="Navigation" press="handleListItemPress" title="{TEAM_NAME}"/>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>