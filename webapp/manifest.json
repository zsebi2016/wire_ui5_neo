{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sapneo.my.wire.wire_ui5_neo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ExpertService": {
				"uri": "/Wire_Service_Team138/odata/v2/ExpertService/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sapneo.my.wire.wire_ui5_neo.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapneo.my.wire.wire_ui5_neo.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ExpertService",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sapneo.my.wire.wire_ui5_neo.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": "TargetNotFound"
				}
			},
			"routes": [
				{
					"name": "appHome",
					"pattern": "",
					"target": [
						"Home"
					]
				},
				{
					"name": "TargetView1",
					"pattern": "teams",
					"target": [
						"TargetView1"
					]
				},
				{
					"name": "ExpertSearch",
					"pattern": "experts",
					"target": [
						"ExpertSearch"
					]
				},
				{
					"name": "Detail",
					"pattern": "detail/{teamId}",
					"target": [
						"Detail"
					]
				},
				{
					"pattern": "Expert/{objectId}",
					"name": "ExpertData",
					"target": [
						"ExpertData"
					]
				}
			],
			"targets": {
				"TargetView1": {
					"viewType": "XML",
					"viewId": "View1",
					"viewName": "View1",
					"viewLevel": 2
				},
				"App": {
					"viewType": "XML",
					"viewName": "App"
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail",
					"viewLevel": 3
				},
				"Home": {
					"viewType": "XML",
					"viewName": "Home",
					"viewLevel": 1
				},
				"TargetNotFound": {
					"viewType": "XML",
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"ExpertSearch": {
					"viewType": "XML",
					"viewName": "ExpertSearch",
					"viewLevel": 2
				},
				"ExpertData": {
					"viewType": "XML",
					"viewName": "ExpertData"
				}
			}
		}
	}
}